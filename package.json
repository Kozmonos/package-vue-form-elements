    "dev:production": "webpack serve --progress --config build/webpack.prod.dev.conf.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test CHROME_BIN=/usr/bin/google-chrome  karma start test/unit/karma.conf.js --capture-timeout=500000 --single-run --log-level=debug",
