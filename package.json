{
  "name": "@kozmonos/form-elements",
  "version": "0.0.3",
  "description": "Vue form-elements component",
  "scripts": {
    "serve": "NODE_ENV=development vue-cli-service serve",
    "serve:production-test": "NODE_ENV=production-test vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name form-elements 'src/main-production.js'",
    "deploy": "custom-command github/push",
    "build:dev": "vue-cli-service build --target app --name form-elements",
    "publish:major": "npm version major --no-git-tag-version;npm run deploy",
    "publish:minor": "npm version minor --no-git-tag-version;npm run deploy",
    "publish:patch": "npm version patch --no-git-tag-version;npm run deploy",
    "test": "vue-cli-service test:unit tests/unit/*.spec.js"
  },
  "main": "dist/form-elements.common.js",
  "unpkg": "dist/form-elements.umd.min.js",
  "files": [
    "dist/**/*",
    "assets/**/*",
    "README.md"
  ],
  "dependencies": {
    "bootstrap-vue": "^2.21.2"
  },
  "devDependencies": {
    "core-js": "^3.21.1",
    "sass": "^1.49.9",
    "sass-loader": "^10.2.1",
    "@vue/cli-service": "~4.5.0",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "@vue/cli-plugin-unit-mocha": "~4.5.13",
    "@vue/test-utils": "^1.0.3",
    "chai": "^4.1.2",
    "null-loader": "^4.0.1",
    "webpack": "^4.46.0"
  },
  "bugs": {
    "url": "hhttps://github.com/Kozmonos/package-vue-form-elements/issues"
  },
  "homepage": "https://github.com/Kozmonos/package-vue-form-elements#readme",
  "keywords": [
    "vue form-elements component",
    "vue form-elements",
    "form-elements component",
    "form-elements"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kozmonos/package-vue-form-elements.git"
  }
}
